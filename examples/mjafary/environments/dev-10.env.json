{
  "name": "dev",
  "description": "Development environment",
  "variables": {
    "API_BASE_URL": "http://localhost:3000"
  },
  "target": {
    "baseUrl": "http://localhost:3000",
    "timeout": 15000
  },
  "load": {
    "phases": [
      {
        "name": "warm-up",
        "duration": "10s",
        "arrivalCount": 10
      }
    ]
  },
  "thresholds": {
    "p95ResponseTime": 500,
    "p99ResponseTime": 1000,
    "maxErrorRate": 0.01
  },
  "http": {
    "pool": 10,
    "timeout": 15000,
    "maxRedirects": 3
  }
}
